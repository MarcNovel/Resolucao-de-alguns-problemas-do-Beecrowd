# Problema 1012 - √Årea

pi = 3.14159
A,B,C = map(float, input().split())

triangulo = A * C / 2
circulo = pi * C ** 2
trapezio = (A + B) * C / 2
quadrado = B ** 2
retangulo = A * B

print('TRIANGULO:', format(triangulo, "<.3f"))
print('CIRCULO:', format(circulo, "<.3f"))
print('TRAPEZIO:', format(trapezio, "<.3f"))
print('QUADRADO:', format(quadrado, "<.3f"))
print('RETANGULO:', format(retangulo, "<.3f"))

# fim de linha